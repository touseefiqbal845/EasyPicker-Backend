
{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static" 
      }
    ],
    "routes": [
      {
        "src": "/products",
        "dest": "index.js"
      },
      {
        "src": "/products/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/categories",
        "dest": "index.js"
      },
      {
        "src": "/categories/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/brands",
        "dest": "index.js"
      },
      {
        "src": "/brands/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/users",
        "dest": "index.js"
      },
      {
        "src": "/users/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/auth",
        "dest": "index.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/cart",
        "dest": "index.js"
      },
      {
        "src": "/cart/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/orders",
        "dest": "index.js"
      },
      {
        "src": "/orders/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/webhook",
        "dest": "index.js"
      },
      {
        "src": "/create-payment-intent",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "build/index.html"
      },
          {
        "src": "/login",
        "dest": "build/index.html"
      },
          {
    "src": "/logout",
    "dest": "build/index.html"
  },
      {
    "src": "/signup",
    "dest": "build/index.html"
  },
      {
    "src": "/products",
    "dest": "build/index.html"
  },
          {
    "src": "/contact-us",
    "dest": "build/index.html"
  },
      {
    "src": "/about-us",
    "dest": "build/index.html"
  },
      {
    "src": "/cart",
    "dest": "build/index.html"
  },
      {
    "src": "/checkout",
    "dest": "build/index.html"
  },
      {
    "src": "/admin",
    "dest": "build/index.html"
  },
      
      {
        "src": "/(.+)",
        "dest": "build/$1"
      }
    ]
  }
